{
    "name": "laravel/cashier-authorize",
    "description": "Laravel Cashier-Authorize provides an expressive, fluent interface to Authorize.net's subscription billing services.",
    "keywords": ["laravel", "stripe", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ales Farcnik",
            "email": "github.ales@farcnik.si"
        },
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/database": "~5.1",
        "illuminate/support": "~5.1",
        "illuminate/container": "^5.2|^5.1",
        "dompdf/dompdf": "^0.8",
        "nesbot/carbon": "~1.0",
        "symfony/http-kernel": "~2.7|~3.0",
        "authorizenet/authorizenet": "^2",
        "jms/serializer": "^1.2",
        "goetas-webservices/xsd2php-runtime":"^0.2.2"
    },
    "require-dev": {
        "goetas-webservices/xsd2php":"^0.3",
        "goetas/xsd-reader": "2.*@dev",
        "illuminate/routing": "~5.1",
        "illuminate/http": "~5.1",
        "illuminate/view": "~5.1",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~6.0",
        "vlucas/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\CashierAuthorizeNet\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    }
}
